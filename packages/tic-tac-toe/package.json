    "edition": "octane"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "license": "MIT",
  "lint-staged": {
    "**/*.{js,ts}": "eslint --cache",
    "**/*.{json,scss}": "prettier --ignore-path '.gitignore'"
  },
  "private": true,
  "resolutions": {
    "broccoli": "3.3.3"
  },
  "scripts": {
    "build": "JOBS=1 NODE_ENV=production ember build --environment=production",
    "build:ci": "run-s prepare build",
    "build:netlify:production": "yarn build",
    "clearContracts": "rm -rf ./build/contracts",
    "contracts:compile": "node ./bin/compile.js",
    "eslint:check": "eslint \"**/*.{js,ts}\" --cache --max-warnings=0",
    "eslint:write": "eslint \"**/*.{js,ts}\" --fix --max-warnings=0",
    "lint:check": "run-s type:check eslint:check prettier:check lint:hbs",
    "lint:hbs": "ember-template-lint .",
    "lint:write": "run-s eslint:write prettier:write",
    "precommit": "lint-staged --quiet",
    "prepare": "run-s clearContracts contracts:compile",
    "prettier:check": "prettier --check '**/*.{scss,json}' --ignore-path '.gitignore'",
    "prettier:write": "prettier --write '**/*.{scss,json}' --ignore-path '.gitignore'",
    "release:netlify:production": "netlify deploy --site $TTT_NETLIFY_ID --dir=dist",
    "start": "run-s contracts:compile start:ember",
    "start:ember": "ember serve",
    "start:shared-ganache": "NODE_ENV=development npx start-shared-ganache",
    "test": "run-s test:contracts test:ember",
    "test:ci": "yarn test:ci:contracts --runInBand",
    "test:ci:contracts": "yarn test:contracts --all --ci --runInBand --bail",
    "test:contracts": "jest -c ./config/jest/jest.contracts.config.js",
    "test:ember": "ember test",
    "test:prepare": "yarn clearContracts && yarn contracts:compile",
    "type:check": "tsc --noEmit"
  }
}
